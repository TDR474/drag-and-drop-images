{
  "name": "drag-and-drop-images",
  "displayName": "Drag and Drop Images",
  "description": "Extension to embed images in Jupyter Notebooks using drag and drop with customization options.",
  "version": "0.0.2",
  "publisher": "frankyywestside-extensions",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "notebook-image-drop.openPanel",
        "title": "Upload and Adjust Image in Notebook Cell"
      },
      {
        "command": "notebook-image-drop.resizeImage",
        "title": "Resize Image in Notebook Cell"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "vscode-test": "^1.6.1"
  },
  "files": [
    "out/**/*",
    "media/**/*"
  ]
}
